{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node":true,
        "browser": true,
        "es6":true,
        "mocha": true
    },
    "rules": {
        "linebreak-style": "off",
        "max-len": "off",
        "comma-dangle": "off",
        "eqeqeq": "off"
    }
}